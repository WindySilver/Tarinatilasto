<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Button minWidth="60.0" mnemonicParsing="false" text="OK">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="60.0" mnemonicParsing="false" text="Peruuta">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
   <center>
      <TableView editable="true" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn minWidth="60.0" prefWidth="-1.0" text="Numero" />
          <TableColumn minWidth="90.0" prefWidth="-1.0" text="Nimi" />
            <TableColumn minWidth="80.0" prefWidth="-1.0" text="Sanamäärä" />
        </columns>
      </TableView>
   </center>
</BorderPane>
